rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /home/{homeId} {
        allow read: if request.auth != null && resource.data.users[request.auth.uid] == true;
        allow write: if true;

        match /items/{itemId} {
            allow read: if true; // TODO
        }
    }
  }
}